<Window x:Class="Bezier.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bezier"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="MainCanvas" HorizontalAlignment="Left" Height="307" Margin="40,88,0,0" VerticalAlignment="Top" Width="716"/>
        <ComboBox x:Name="ExampleSelection" HorizontalAlignment="Left" Margin="40,37,0,0" VerticalAlignment="Top" Width="184" SelectionChanged="ComboBox_SelectionChanged">
            <ComboBoxItem Content="Rough Draw Example" IsSelected="True"/>
        </ComboBox>
    </Grid>
</Window>
